<mvc:View controllerName="ck.expert.controller.Expert" 
  xmlns:mvc="sap.ui.core.mvc" 
  xmlns="sap.m" 
  xmlns:control="ck.expert.control" 
  xmlns:indicator="ck.expert.control.indicator">
  <VBox>
    <control:CandlestickChart candles="{Rows}"
      padding="60"
      height="480">
      <control:candles>
        <control:Candle x="{time}"
          open="{open}"
          high="{high}"
          low="{low}"
          close="{close}"/>
      </control:candles>
    </control:CandlestickChart>
